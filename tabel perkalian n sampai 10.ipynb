    "# Menginput Angka\n",
    "angka = int(input(\"Menampilkan Tabel Perkalian dari: \"))\n",
    " \n",
    "# Menghitung 10 Kali dari Kisaran 1 sampai 10\n",
    "for i in range(1, 11):\n",
    " \n",
    "#Menampilkan Tabel Perkalian\n",
    "    print(f\"{angka} x {i} = {angka*i}\")"
